{
  "name": "server",
  "version": "0.1.0",
  "main": "server.js",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "pg": "^8.5.1",
